{
    "family": "auth-service-task",
    "containerDefinitions": [
        {
          "name": "Auth-container",
          "image": "762502711875.dkr.ecr.ap-south-1.amazonaws.com/nest-auth:latest",
          "cpu": 0,
          "portMappings": [
            {
              "name": "auth-container-3001-tcp",
              "containerPort": 3001,
              "hostPort": 3001,
              "protocol": "tcp",
              "appProtocol": "http"
            }
          ],
          "essential": true,
          "environment": [
            {
              "name": "HTTP_PORT",
              "value": "3001"
            },
            {
              "name": "JWT_EXPIRATION",
              "value": "3600"
            },
            {
              "name": "TCP_PORT",
              "value": "3002"
            }
          ],
          "secrets": [
            {
              "name": "MONGODB_URI",
              "valueFrom": "arn:aws:secretsmanager:ap-south-1:762502711875:secret:Nest-microservice-Secret-Xyxai7/MONGODB_URI"
            },
            {
              "name": "JWT_SECRET",
              "valueFrom": "arn:aws:secretsmanager:ap-south-1:762502711875:secret:Nest-microservice-Secret-Xyxai7/JWT_SECRET"
            }
          ],
          "environmentFiles": [],
          "mountPoints": [],
          "volumesFrom": [],
          "ulimits": [],
          "logConfiguration": {
            "logDriver": "awslogs",
            "options": {
              "awslogs-group": "/ecs/auth-service-task",
              "awslogs-create-group": "true",
              "awslogs-region": "ap-south-1",
              "awslogs-stream-prefix": "ecs"
            },
            "secretOptions": []
          },
          "systemControls": []
        }
    ],
    "executionRoleArn": "arn:aws:iam::762502711875:role/ecsTaskExecutionRole",
    "networkMode": "awsvpc",
    "volumes": [],
    "placementConstraints": [],
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "cpu": "1024",
    "memory": "3072",
    "runtimePlatform": {
        "cpuArchitecture": "X86_64",
        "operatingSystemFamily": "LINUX"
    }
}